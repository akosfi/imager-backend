{
  "family": "imager",
  "executionRoleArn": "arn:aws:iam::120646787562:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "imager-backend",
      "image": "image",
      "cpu": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "memory": 1024,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/imager",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}